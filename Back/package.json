{
  "name": "rickmorty-api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "db:migrate": "sequelize db:migrate",
    "db:undo": "sequelize db:migrate:undo",
    "db:reset": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo:all"
  },
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^8.1.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "redis": "^5.8.2",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5",
    "@types/express-serve-static-core": "^5.0.7",
    "@types/node": "^24.3.0",
    "sequelize-cli": "^6.6.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}